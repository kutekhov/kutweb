<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kutweb.entities.OptionsEntity" table="options" schema="e_telecom">
        <id name="idOptions" column="id_options"/>
        <property name="nameOptions" column="name_options"/>
        <property name="priceOptions" column="price_options"/>
        <property name="costConnection" column="cost_connection"/>
    </class>
</hibernate-mapping>